<div class="container container-profile">

  <div class="col-md-3">
    <%= render 'layouts/user_bar' %>
  </div>
  <div class="col-md-9">
    <%= render 'layouts/tabs' %>

    <div class="feed">
      <h3 class="contribution-activity">Contribution Activity</h3>
        <% @user_commits.each do |commit| %>
          <li>
            <%= "Created commit in #{commit.repo_name}" %><br>
            <%= "Message: #{commit.message_hash.first[:message]}" %>
            <hr>
          </li>
        <% end %>
      </div>
  </div>
</div>
